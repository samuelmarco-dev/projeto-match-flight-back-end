{
    "name": "match-flight-back-end",
    "version": "1.0.0",
    "description": "",
    "engines": {
        "node": "16.x",
        "npm": "8.x"
    },
    "type": "module",
    "main": "server.js",
    "scripts": {
        "build": "tsc",
        "start": "node ./dist/server.js",
        "dev": "nodemon src/server.ts",
        "dev:test": "dotenv -e .test.env nodemon src/server.ts",
        "test": "dotenv -e .test.env prisma migrate dev && NODE_OPTIONS=--experimental-vm-modules dotenv -e .test.env jest -- -i --coverage",
        "test:integration": "dotenv -e .test.env prisma migrate dev && NODE_OPTIONS=--experimental-vm-modules dotenv -e .test.env jest -- -i --testPathPattern=tests/integration",
        "test:unit": "dotenv -e .test.env prisma migrate dev && NODE_OPTIONS=--experimental-vm-modules dotenv -e .test.env jest -- -i --coverage --testPathPattern=tests/unitary"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^4.1.1",
        "@types/yargs": "^17.0.11",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dayjs": "^1.11.4",
        "dotenv": "^16.0.1",
        "electron-updater": "^4.2.0",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "helmet": "^5.1.1",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.3.0",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.5.0",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.3.3",
        "@types/supertest": "^2.0.11",
        "dotenv-cli": "^6.0.0",
        "jest": "^28.1.3",
        "nodemon": "^2.0.19",
        "prisma": "^4.1.1",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
